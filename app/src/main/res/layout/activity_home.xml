                               android:layout_height="wrap_content"
                               android:fontFamily="@font/archivo_regular"
                               android:textSize="14sp"
                               android:text="@string/home_bot_min_title"
                               android:textColor="@color/white"
                               android:textAllCaps="true" />
                           <TextView
