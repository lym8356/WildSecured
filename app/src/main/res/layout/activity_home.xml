                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/archivo_regular"
                                android:textColor="@color/primary_orange"
                                android:text="@string/home_bot_min_title"
                                android:padding="2dp"/>
                            <TextView
                                android:layout_width="wrap_content"
